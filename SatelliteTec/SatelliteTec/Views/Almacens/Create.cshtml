@model SatelliteTec.Models.Almacen

@{
    ViewBag.Title = "Crear Almacen";
}






<div style="margin-top: 50px">
    <div class="shadow p-3 mb-5 bg-white rounded">

        <h2 style="text-align: center">@ViewBag.Title</h2>

    </div>


    <div class="card">
        <h5 class="card-header">

            <label style="cursor: pointer;" onclick="location.href='@Url.Action("Index", "Almacens")'"><i class="far fa-hand-point-left fa-lg"></i> Regresar a la Lista</label>
        </h5>
        <div class="card-body">
            <h5 class="card-title">Puede crear un Proveedor</h5>



@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Almacen</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.Estado, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Estado, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Estado, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

        </div>
    </div>

</div>









@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
